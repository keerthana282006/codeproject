class Solution {
    public String reverseWords(String s) {
        String [] words=s.trim().split("\\s+");
        StringBuilder k=new StringBuilder();
        for(int i=words.length-1;i>=0;i--){
            k.append(words[i]);
            if(i>0){
                k.append(" ");
            }
        }
        return k.toString();
    }
}
