function greeting(parameterVariable) {
    // This line is already provided in the editor
    console.log("Hello, World!");

    // Print the contents of parameterVariable
    console.log(parameterVariable);
}


2.DATA TYPES

function performOperation(secondInteger, secondDecimal, secondString) {
    // Variables already declared
    const firstInteger = 4;
    const firstDecimal = 4.0;
    const firstString = 'HackerRank ';

    // Convert and add integer
    console.log(firstInteger + Number(secondInteger));

    // Convert and add decimal
    console.log(firstDecimal + Number(secondDecimal));

    // Concatenate strings
    console.log(firstString + secondString);
}


3. ARITHMETIC OPERATIONS

function getArea(length, width) {
    // Calculate area
    return length * width;
}

function getPerimeter(length, width) {
    // Calculate perimeter
    return 2 * (length + width);
}


4. FUNCTIONS

function factorial(n) {
    let result = 1;

    for (let i = 1; i <= n; i++) {
        result *= i;
    }

    return result;
}

5. LET AND CONST KEYWORDS

function main() {
    const PI = Math.PI;

    const r = parseFloat(readLine());

    const area = PI * r * r;
    const perimeter = 2 * PI * r;

    console.log(area);
    console.log(perimeter);
}

6. IF ELSE STATEMENT

function getGrade(score) {
    let grade;

    if (score > 25) {
        grade = 'A';
    } else if (score > 20) {
        grade = 'B';
    } else if (score > 15) {
        grade = 'C';
    } else if (score > 10) {
        grade = 'D';
    } else if (score > 5) {
        grade = 'E';
    } else {
        grade = 'F';
    }

    return grade;
}
11. THROW AND ERROR

function isPositive(a) {
    if (a > 0) {
        return "YES";
    } else if (a === 0) {
        throw new Error("Zero Error");
    } else {
        throw new Error("Negative Error");
    }
}
12. OBJECTS

function Rectangle(a, b) {
    return {
        length: a,
        width: b,
        perimeter: 2 * (a + b),
        area: a * b
    };
}17. ARROW FUNCTIONS

const modifyArray = (nums) => {
    return nums.map(n => (n % 2 === 0 ? n * 2 : n * 3));
};21. REGULAR EXPRESSION II

function regexVar() {
    return /^(Mr|Mrs|Ms|Dr|Er)\.[a-zA-Z]+$/;
}
24. BUTTON CONTAINER


<!DOCTYPE html>
<html>
<head>
    <style>
        #btns {
            width: 75%;
        }

        #btns button {
            width: 30%;
            height: 48px;
            font-size: 24px;
        }
    </style>
</head>

<body>
    <div id="btns">
        <button id="btn1">1</button>
        <button id="btn2">2</button>
        <button id="btn3">3</button>
        <br>
        <button id="btn4">4</button>
        <button id="btn5">5</button>
        <button id="btn6">6</button>
        <br>
        <button id="btn7">7</button>
        <button id="btn8">8</button>
        <button id="btn9">9</button>
    </div>

    <script>
        const btn5 = document.getElementById("btn5");

        btn5.onclick = function () {
            const values = [
                btn1.innerHTML,
                btn2.innerHTML,
                btn3.innerHTML,
                btn6.innerHTML,
                btn9.innerHTML,
                btn8.innerHTML,
                btn7.innerHTML,
                btn4.innerHTML
            ];

            btn1.innerHTML = values[7];
            btn2.innerHTML = values[0];
            btn3.innerHTML = values[1];
            btn6.innerHTML = values[2];
            btn9.innerHTML = values[3];
            btn8.innerHTML = values[4];
            btn7.innerHTML = values[5];
            btn4.innerHTML = values[6];
        };
    </script>
</body>
</html>
